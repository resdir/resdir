{
  "_test": {
    "@type": "macro",
    "@expressions": [
      "./packages/util @test",
      "./packages/version @test",
      "./packages/version-range @test",
      "./resources/js/dependencies @test",
      "./resources/js/package @test",
      "./resources/js/transpiler @test"
    ],
    "@listen": "after:@test",
    "@hidden": "true"
  },
  "publish": {
    "@type": "macro",
    "@expressions": [
      "./resources/js/dependencies @publish",
      "./resources/js/resource @publish",
      "./resources/js/jest @publish",
      "./resources/js/package @publish",
      "./resources/js/transpiler @publish",
      "./resources/js/esnext-resource @publish",
      "./resources/js/esnext-jest @publish",
      "./resources/js/esnext-package @publish"
    ]
  }
}
