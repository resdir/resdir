{
  "@load": "./@resource.json",
  "contentDirectory": "./builds/dev",
  "testServer": {
    "port": 9012
  },
  "dependencies": {
    "color": "^3.0.0",
    "hast-util-sanitize": "^1.1.2",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "radium": "^0.21.2",
    "radium-starter": "^0.10.0",
    "react-lowlight": "^1.1.1",
    "react-md-spinner": "^0.2.5",
    "react-router-dom": "^4.2.2",
    "remark": "^9.0.0",
    "remark-react": "^4.0.1",
    "@resdir/http-client": "^0.2.0",
    "@resdir/react-modal": "^0.1.0",
    "sleep-promise": "^2.0.0",
    "url-search-params": "^0.10.0"
  },
  "bundler": {
    "output": "./dist/bundle.dev.js"
  },
  "copier": {
    "destinationDirectory": "./builds/dev",
    "clearDestination": true,
    "files": [
      {
        "source": "./index.html",
        "replacements": {
          "${REACT_URL}": "https://unpkg.com/react@16.2.0/umd/react.development.js",
          "${REACT_DOM_URL}": "https://unpkg.com/react-dom@16.2.0/umd/react-dom.development.js"
        }
      },
      {
        "source": "./dist/bundle.dev.js",
        "destination": "./bundle.js",
        "replacements": {
          "${RESDIR_REGISTRY_SERVER}": "http://registry.dev.resdir.com",
          "${RUN_WEBSITE_URL}": "http://dev.run.tools"
        }
      },
      "./images"
    ]
  },
  "freezer": {
    "sourceDirectory": "./builds/dev"
  }
}
