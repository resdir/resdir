{
  _test: {
    "@type": "macro",
    "@hidden": "true",
    "@listen": "after:@test",
    "@expression": [
      "./packages/util @test",
      "./packages/version @test",
      "./packages/version-range @test",
      "./resources/js/dependencies @test",
      "./resources/js/package @test",
      "./resources/js/transpiler @test"
    ]
  },
  publish: {
    "@type": "macro",
    "@expression": [
      "./resources/resdir/resource publish --patch",
      "./resources/resdir/registry publish --patch",
      "./resources/resdir/example publish --patch",
      "./resources/js/dependencies publish --patch",
      "./resources/js/resource publish --patch",
      "./resources/js/jest publish --patch",
      "./resources/js/package publish --patch",
      "./resources/js/transpiler publish --patch",
      "./resources/js/esnext-resource publish --patch",
      "./resources/js/esnext-jest publish --patch",
      "./resources/js/esnext-package publish --patch"
    ]
  }
}
