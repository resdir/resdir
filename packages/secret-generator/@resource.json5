{
  "@import": "js/esnext-package",
  name: "@resdir/secret-generator",
  version: "1.0.3",
  description: "Generate a secret",
  files: [
    "./dist"
  ],
  main: "./dist/index.js",
  dependencies: [
    "nanoid@^0.2.0"
  ],
  transpiler: {
    targets: {
      node: 6
    }
  },
  tests: {
    "@import": "js/esnext-jest",
    "@directory": "./tests",
    dependencies: [
      "lodash@^4.17.4"
    ],
    testEnvironment: "node",
    run: {
      "@listen": "after:@test"
    }
  }
}