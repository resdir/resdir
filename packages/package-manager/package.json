{
  "name": "@resdir/package-manager",
  "version": "0.1.0",
  "description": "Abstract node package managers (currently using yarn)",
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/resdir/resdir"
  },
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "sourceMaps": "inline"
  },
  "dependencies": {
    "child-process-promise": "^2.2.1",
    "lodash": "^4.17.4",
    "yarn": "^0.24.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0"
  }
}
