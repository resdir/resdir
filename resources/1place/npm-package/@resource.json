{
  "@import": "resdir/resource",
  "id": "1place/npm-package",
  "version": "0.1.8",
  "isPublic": true,
  "authors": [
    "Manuel Vila <hi@mvila.me>"
  ],
  "license": "MIT",
  "repository": "https://github.com/resdir/resdir.git",
  "@export": {
    "@import": "js/npm-package",
    "@name": "1Place's npm Package",
    "@description": "1Place's starting point for implementing npm packages",
    "files": [
      "./dist"
    ],
    "main": "./dist/cjs",
    "module": "./dist/esm",
    "cjsTranspiler": {
      "@import": "js/transpiler",
      "format": "cjs",
      "run": {
        "@listen": [
          "@build",
          "@fileModified"
        ]
      }
    },
    "esmTranspiler": {
      "@import": "js/transpiler",
      "format": "esm",
      "run": {
        "@listen": [
          "@build",
          "@fileModified"
        ]
      }
    }
  }
}
