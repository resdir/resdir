{
  "@import": "resdir/resource#^0.1.0",
  "id": "1place/js-repository",
  "version": "0.1.9",
  "isPublic": true,
  "@export": {
    "_jsRepositoryInstall": {
      "@type": "method",
      "@isHidden": true,
      "@run": "(@import shell/bash) run \"git config core.hooksPath .githooks\"",
      "@listen": "@install"
    },
    "_jsRepositoryLint": {
      "@type": "method",
      "@isHidden": true,
      "@run": [
        "(@import shell/bash) run \"./node_modules/.bin/eslint .\"",
        "(@import shell/bash) run \"./node_modules/.bin/prettier --check '**/*.js'\""
      ],
      "@listen": "@lint"
    },
    "_jsRepositoryDependencies": {
      "@comment": "TODO: Remove 'indent-string' dependency (https://github.com/prettier/prettier-eslint-cli/issues/169)",
      "@import": "js/npm-dependencies#^0.1.0",
      "@isHidden": true,
      "@value": {
        "@1place/eslint-config-1place": "~0.1.9",
        "eslint": "~6.8.0",
        "eslint-plugin-react": "~7.17.0",
        "prettier": "~1.19.1"
      }
    }
  }
}
