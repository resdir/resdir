{
  "@import": [
    "resdir/resource",
    "js/resource"
  ],
  "id": "1place/jest",
  "version": "0.1.28",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "jest": "^21.2.1",
      "regenerator-runtime": "^0.11.0",
      "@resdir/console": "^0.1.0",
      "@resdir/process-manager": "^0.1.0"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler"
    }
  },
  "@export": {
    "@description": "Test suite (using Jest)",
    "@implementation": "./dist",
    "dependencies": {
      "@import": "js/npm-dependencies"
    },
    "roots": {
      "@type": "array"
    },
    "testEnvironment": {
      "@type": "string"
    },
    "run": {
      "@type": "method",
      "@input": {
        "testPathPattern": {
          "@type": "string",
          "@position": 0,
          "@isOptional": true
        }
      },
      "@listen": "@test"
    }
  }
}
