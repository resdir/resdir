{
  "@import": [
    "resdir/resource",
    "js/resource"
  ],
  "id": "resdir/registry",
  "version": "0.1.12",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "@resdir/console": "^0.1.0",
      "@resdir/registry-client": "^0.1.13"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler"
    }
  },
  "@export": {
    "@implementation": "./dist",
    "signUp": {
      "@type": "method",
      "@input": {
        "email": {
          "@type": "string",
          "@position": 0
        }
      }
    },
    "signIn": {
      "@type": "method",
      "@input": {
        "email": {
          "@type": "string",
          "@position": 0
        }
      }
    },
    "signOut": {
      "@type": "method"
    },
    "user": {
      "@implementation": "./dist/user",
      "show": {
        "@type": "method"
      },
      "delete": {
        "@type": "method"
      },
      "organizations": {
        "@aliases": [
          "organization",
          "orgs",
          "org"
        ],
        "@implementation": "./dist/user-organizations",
        "list": {
          "@type": "method",
          "@aliases": [
            "ls"
          ]
        }
      },
      "namespace": {
        "@aliases": [
          "ns"
        ],
        "@implementation": "./dist/user-namespace",
        "add": {
          "@type": "method",
          "@input": {
            "namespace": {
              "@type": "string",
              "@aliases": [
                "ns"
              ],
              "@position": 0
            }
          }
        },
        "remove": {
          "@type": "method",
          "@aliases": [
            "rm"
          ]
        }
      },
      "github": {
        "@aliases": [
          "gh"
        ],
        "@implementation": "./dist/user-github-account",
        "connect": {
          "@type": "method"
        },
        "disconnect": {
          "@type": "method"
        }
      }
    },
    "organizations": {
      "@aliases": [
        "organization",
        "orgs",
        "org"
      ],
      "@implementation": "./dist/organizations",
      "create": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0
          }
        }
      },
      "delete": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0
          }
        }
      },
      "addMember": {
        "@type": "method",
        "@input": {
          "organizationNamespace": {
            "@type": "string",
            "@aliases": [
              "organization",
              "org"
            ],
            "@position": 0
          },
          "userNamespace": {
            "@type": "string",
            "@aliases": [
              "user"
            ],
            "@position": 1
          }
        }
      },
      "removeMember": {
        "@type": "method",
        "@aliases": [
          "rmMember"
        ],
        "@input": {
          "organizationNamespace": {
            "@type": "string",
            "@aliases": [
              "organization",
              "org"
            ],
            "@position": 0
          },
          "userNamespace": {
            "@type": "string",
            "@aliases": [
              "user"
            ],
            "@position": 1
          }
        }
      }
    },
    "communities": {
      "@aliases": [
        "community"
      ],
      "@implementation": "./dist/communities",
      "create": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0
          }
        }
      },
      "delete": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0
          }
        }
      }
    }
  }
}
