{
  "@import": "js/esnext-resource",
  id: "resdir/registry",
  version: "0.1.0",
  files: [
    "./dist"
  ],
  dependencies: [
    "@resdir/console@^0.1.0",
    "@resdir/registry-client@^0.1.13"
  ],
  transpiler: {
    targets: {
      node: 8
    }
  },
  "@export": {
    "@implementation": "./dist",
    signUp: {
      "@type": "method",
      "@parameters": {
        email: {"@type": "string", "@position": 0}
      }
    },
    signIn: {
      "@type": "method",
      "@parameters": {
        email: {"@type": "string", "@position": 0}
      }
    },
    signOut: {
      "@type": "method"
    },
    user: {
      "@implementation": "./dist/user",
      show: {
        "@type": "method"
      },
      delete: {
        "@type": "method"
      },
      organizations: {
        "@aliases": ["organization", "orgs", "org"],
        "@implementation": "./dist/user-organizations",
        list: {
          "@aliases": ["ls"],
          "@type": "method"
        }
      },
      namespace: {
        "@aliases": ["ns"],
        "@implementation": "./dist/user-namespace",
        add: {
          "@type": "method",
          "@parameters": {
            namespace: {"@type": "string", "@aliases": ["ns"], "@position": 0}
          }
        },
        remove: {
          "@aliases": ["rm"],
          "@type": "method"
        }
      },
      github: {
        "@aliases": ["gh"],
        "@implementation": "./dist/user-github-account",
        connect: {
          "@type": "method",
        },
        disconnect: {
          "@type": "method"
        }
      }
    },
    organizations: {
      "@aliases": ["organization", "orgs", "org"],
      "@implementation": "./dist/organizations",
      create: {
        "@type": "method",
        "@parameters": {
          namespace: {"@type": "string", "@aliases": ["ns"], "@position": 0}
        }
      },
      delete: {
        "@type": "method",
        "@parameters": {
          namespace: {"@type": "string", "@aliases": ["ns"], "@position": 0}
        }
      },
      addMember: {
        "@type": "method",
        "@parameters": {
          organizationNamespace: {"@type": "string", "@aliases": ["organization", "org"], "@position": 0},
          userNamespace: {"@type": "string", "@aliases": ["user"], "@position": 1}
        }
      },
      removeMember: {
        "@type": "method",
        "@aliases": ["rmMember"],
        "@parameters": {
          organizationNamespace: {"@type": "string", "@aliases": ["organization", "org"], "@position": 0},
          userNamespace: {"@type": "string", "@aliases": ["user"], "@position": 1}
        }
      }
    }
  }
}
