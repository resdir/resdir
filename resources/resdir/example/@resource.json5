{
  "@import": "js/esnext-resource",
  "@parameters": {
    color: "green"
  },
  "@implementation": "./dist",
  id: "resdir/example",
  version: "0.3.11",
  files: [
    "./dist"
  ],
  dependencies: [
    "chalk@^2.1.0"
  ],
  hello: {
    "@type": "method",
    "@listen": "hi"
  },
  "@private": {
    tests: {
      "@import": "js/esnext-jest",
      "@directory": "./tests",
      testEnvironment: "node",
      run: {
        "@listen": "after:@test"
      }
    }
  }
}