{
  "@import": [
    "resdir/resource",
    "js/resource"
  ],
  "@implementation": "./dist",
  "id": "resdir/example",
  "version": "0.3.44",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "chalk": "^2.1.0"
    }
  },
  "hello": {
    "@type": "method",
    "@description": "Print a nice greeting",
    "@aliases": [
      "hi"
    ],
    "@examples": [
      "hello",
      "hi --color=red"
    ],
    "@input": {
      "color": {
        "@default": "green"
      }
    },
    "@listen": "hi"
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler"
    },
    "tests": {
      "@import": "1place/esnext-jest",
      "@directory": "./tests"
    }
  }
}
