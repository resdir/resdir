{
  "@import": [
    "resdir/resource",
    "1place/js-resource"
  ],
  "@implementation": "./dist/cjs",
  "@description": "A simple 'Hello, World!' example",
  "id": "resdir/example",
  "version": "0.3.51",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "chalk": "^2.1.0",
    "@resdir/console": "^0.1.35"
  },
  "hello": {
    "@type": "method",
    "@description": "Print a nice greeting",
    "@aliases": [
      "hi"
    ],
    "@examples": [
      "hello",
      "hi --color=red"
    ],
    "@input": {
      "color": {
        "@default": "green",
        "@description": "The color of the printed message (using 'chalk')"
      }
    }
  }
}
