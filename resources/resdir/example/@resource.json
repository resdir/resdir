{
  "@import": [
    "resdir/resource#^0.1.0",
    "1place/js-resource#^0.1.0"
  ],
  "@name": "Resource Example",
  "@description": "A simple 'Hello, World!' example",
  "@implementation": "./dist/cjs",
  "id": "resdir/example",
  "version": "0.3.53",
  "isPublic": true,
  "authors": [
    "Manuel Vila <hi@mvila.me>"
  ],
  "license": "MIT",
  "repository": "https://github.com/resdir/resdir.git",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "chalk": "^2.1.0",
    "@resdir/console": "^0.1.35"
  },
  "hello": {
    "@type": "method",
    "@description": "Print a nice greeting",
    "@aliases": [
      "hi"
    ],
    "@examples": [
      "hello",
      "hi --color=red"
    ],
    "@input": {
      "color": {
        "@description": "The color of the printed message (using 'chalk')",
        "@default": "green"
      }
    }
  }
}
