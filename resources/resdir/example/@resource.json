{
  "@import": "resdir/resource",
  "@implementation": "./dist",
  "id": "resdir/example",
  "version": "0.3.45",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "chalk": "^2.1.0",
      "@resdir/console": "^0.1.35"
    }
  },
  "hello": {
    "@type": "method",
    "@description": "Print a nice greeting",
    "@aliases": [
      "hi"
    ],
    "@examples": [
      "hello",
      "hi --color=red"
    ],
    "@input": {
      "color": {
        "@default": "green"
      }
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler"
    }
  }
}
