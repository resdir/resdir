{
  "@import": [
    "./",
    "js/resource"
  ],
  "id": "resdir/resource",
  "version": "0.1.14",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "@resdir/console": "^0.1.0",
      "@resdir/gitignore-manager": "^0.1.0",
      "@resdir/registry-client": "^0.1.13",
      "@resdir/resource-identifier": "^0.1.0",
      "@resdir/version": "^0.1.0"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "resdir/esnext-transpiler"
    }
  },
  "@export": {
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist/resource.js",
    "id": {
      "@type": "string"
    },
    "version": {
      "@type": "string",
      "@implementation": "./dist/version-property.js",
      "bump": {
        "@type": "method",
        "@parameters": {
          "major": false,
          "minor": false,
          "patch": false
        }
      }
    },
    "name": {
      "@type": "string"
    },
    "description": {
      "@type": "string"
    },
    "keywords": {
      "@type": "array"
    },
    "homepage": {
      "@type": "string"
    },
    "repository": {
      "@type": "string"
    },
    "files": {
      "@type": "array"
    },
    "publish": {
      "@type": "method",
      "@parameters": {
        "major": false,
        "minor": false,
        "patch": false
      }
    },
    "@initialize": {
      "@type": "method",
      "@parameters": {
        "id": {
          "@type": "string",
          "@position": 0
        },
        "version": {
          "@type": "string",
          "@aliases": [
            "v"
          ]
        },
        "gitignore": true
      }
    }
  }
}
