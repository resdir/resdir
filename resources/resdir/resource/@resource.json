{
  "@import": [
    "./",
    "js/resource"
  ],
  "id": "resdir/resource",
  "version": "0.1.20",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "@resdir/console": "^0.1.0",
      "@resdir/gitignore-manager": "^0.1.0",
      "@resdir/registry-client": "^0.1.13",
      "@resdir/resource-identifier": "^0.1.0",
      "@resdir/version": "^0.1.0"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler"
    },
    "tests": {
      "@import": "1place/esnext-jest",
      "@directory": "./tests",
      "dependencies": {
        "run-core": "^0.10.0"
      }
    }
  },
  "@export": {
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist",
    "id": {
      "@type": "string",
      "@implementation": "./dist/id-property.js",
      "getNamespace": {
        "@type": "method"
      },
      "getName": {
        "@type": "method"
      },
      "validate": {
        "@type": "method",
        "@parameters": {
          "throwIfInvalid": true
        }
      }
    },
    "version": {
      "@type": "string",
      "@implementation": "./dist/version-property.js",
      "validate": {
        "@type": "method",
        "@parameters": {
          "throwIfInvalid": true
        }
      },
      "bump": {
        "@type": "method",
        "@parameters": {
          "major": false,
          "minor": false,
          "patch": false
        }
      }
    },
    "name": {
      "@type": "string"
    },
    "description": {
      "@type": "string"
    },
    "keywords": {
      "@type": "array"
    },
    "homepage": {
      "@type": "string"
    },
    "repository": {
      "@type": "string"
    },
    "files": {
      "@type": "array"
    },
    "publish": {
      "@type": "method",
      "@parameters": {
        "major": false,
        "minor": false,
        "patch": false
      }
    },
    "initialize": {
      "@type": "method",
      "@parameters": {
        "id": {
          "@type": "string",
          "@position": 0
        },
        "version": {
          "@type": "string",
          "@aliases": [
            "v"
          ]
        },
        "gitignore": true
      },
      "@listen": "@created"
    }
  }
}
