        "@type": "method"
      },
      "delete": {
        "@type": "method"
      },
      "organizations": {
        "@aliases": [
          "organization",
          "orgs",
          "org"
        ],
        "@implementation": "./dist/user-organizations",
        "list": {
          "@type": "method",
          "@aliases": [
            "ls"
          ]
        }
      },
      "namespace": {
        "@aliases": [
          "ns"
        ],
        "@implementation": "./dist/user-namespace",
        "create": {
          "@type": "method",
          "@input": {
            "namespace": {
              "@type": "string",
              "@aliases": [
                "ns"
              ],
              "@position": 0,
              "@isOptional": true
            },
            "permissionToken": {
              "@type": "string",
              "@isOptional": true
            }
          }
        },
        "delete": {
          "@type": "method"
        }
      },
      "gitHub": {
        "@aliases": [
          "github",
          "gh"
        ],
        "@implementation": "./dist/user-github-account",
        "connect": {
          "@type": "method",
          "@input": {
            "parentAction": {
              "@type": "string",
              "@isOptional": true
            }
          }
        },
        "disconnect": {
          "@type": "method"
        }
      }
    },
    "organizations": {
      "@aliases": [
        "organization",
        "orgs",
        "org"
      ],
      "@implementation": "./dist/organizations",
      "create": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0,
            "@isOptional": true
          },
          "permissionToken": {
            "@type": "string",
            "@isOptional": true
          }
        }
      },
      "delete": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0,
            "@isOptional": true
          }
        }
      },
      "addMember": {
        "@type": "method",
        "@input": {
          "organizationNamespace": {
            "@type": "string",
            "@aliases": [
              "organization",
              "org"
            ],
            "@position": 0,
            "@isOptional": true
          },
          "userNamespace": {
            "@type": "string",
            "@aliases": [
              "user"
            ],
            "@position": 1,
            "@isOptional": true
          }
        }
      },
      "removeMember": {
        "@type": "method",
        "@aliases": [
          "rmMember"
        ],
        "@input": {
          "organizationNamespace": {
            "@type": "string",
            "@aliases": [
              "organization",
              "org"
            ],
            "@position": 0,
            "@isOptional": true
          },
          "userNamespace": {
            "@type": "string",
            "@aliases": [
              "user"
            ],
            "@position": 1,
            "@isOptional": true
          }
        }
      }
    },
    "communities": {
      "@aliases": [
        "community"
      ],
      "@implementation": "./dist/communities",
      "create": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0,
            "@isOptional": true
          },
          "permissionToken": {
            "@type": "string",
            "@isOptional": true
          }
        }
      },
      "delete": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0,
            "@isOptional": true
          }
        }
      }
    },
