          "@position": 1,
          "@isOptional": true
        },
        "permissionToken": {
          "@type": "string",
          "@isOptional": true
        }
      }
    },
    "signIn": {
      "@type": "method",
      "@input": {
        "email": {
          "@type": "string",
          "@position": 0,
          "@isOptional": true
        }
      }
    },
    "signOut": {
      "@type": "method"
    },
    "user": {
      "@implementation": "./dist/user",
