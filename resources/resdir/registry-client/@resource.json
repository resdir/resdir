{
  "@import": [
    "resdir/resource",
    "js/resource"
  ],
  "id": "resdir/registry-client",
  "version": "0.1.18",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "debug": "^3.1.0",
    "fs-extra": "^5.0.0",
    "hasha": "^3.0.0",
    "is-directory": "^0.3.1",
    "lodash": "^4.17.4",
    "opn": "^5.2.0",
    "recursive-readdir": "^2.2.1",
    "@resdir/archive-manager": "^0.1.4",
    "@resdir/console": "^0.1.0",
    "@resdir/expression": "^0.1.2",
    "@resdir/file-manager": "^0.1.4",
    "@resdir/http-client": "^0.2.8",
    "@resdir/information": "^0.1.5",
    "@resdir/resource-specifier": "^0.1.14",
    "@resdir/secret-generator": "^1.0.4"
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "js/transpiler",
      "run": {
        "@listen": [
          "@build",
          "@fileModified"
        ]
      }
    }
  },
  "@export": {
    "@implementation": "./dist/cjs",
    "registryServer": {
      "@type": "string",
      "@isHidden": true
    },
    "uploadServer": {
      "@type": "object",
      "@isHidden": true
    },
    "ping": {
      "@type": "method",
      "@isHidden": true
    },
    "signUp": {
      "@type": "method",
      "@input": {
        "email": {
          "@type": "string",
          "@position": 0,
          "@isOptional": true
        },
        "namespace": {
          "@type": "string",
          "@aliases": [
            "ns"
          ],
          "@position": 1,
          "@isOptional": true
        },
        "permissionToken": {
          "@type": "string",
          "@isOptional": true
        }
      }
    },
    "signIn": {
      "@type": "method",
      "@input": {
        "email": {
          "@type": "string",
          "@position": 0,
          "@isOptional": true
        }
      }
    },
    "signOut": {
      "@type": "method"
    },
    "currentUser": {
      "@aliases": [
        "me"
      ],
      "@implementation": "./dist/cjs/current-user",
      "show": {
        "@type": "method"
      },
      "delete": {
        "@type": "method"
      },
      "organizations": {
        "@aliases": [
          "organization",
          "orgs",
          "org"
        ],
        "@implementation": "./dist/cjs/current-user-organizations",
        "list": {
          "@type": "method",
          "@aliases": [
            "ls"
          ]
        }
      },
      "namespace": {
        "@aliases": [
          "ns"
        ],
        "@implementation": "./dist/cjs/current-user-namespace",
        "create": {
          "@type": "method",
          "@input": {
            "namespace": {
              "@type": "string",
              "@aliases": [
                "ns"
              ],
              "@position": 0,
              "@isOptional": true
            },
            "permissionToken": {
              "@type": "string",
              "@isOptional": true
            }
          }
        },
        "delete": {
          "@type": "method"
        }
      },
      "gitHub": {
        "@aliases": [
          "github",
          "gh"
        ],
        "@implementation": "./dist/cjs/current-user-github-account",
        "connect": {
          "@type": "method",
          "@input": {
            "parentAction": {
              "@type": "string",
              "@isOptional": true
            }
          }
        },
        "disconnect": {
          "@type": "method"
        }
      }
    },
    "organizations": {
      "@aliases": [
        "organization",
        "orgs",
        "org"
      ],
      "@implementation": "./dist/cjs/organizations",
      "create": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0,
            "@isOptional": true
          },
          "permissionToken": {
            "@type": "string",
            "@isOptional": true
          }
        }
      },
      "delete": {
        "@type": "method",
        "@input": {
          "namespace": {
            "@type": "string",
            "@aliases": [
              "ns"
            ],
            "@position": 0,
            "@isOptional": true
          }
        }
      },
      "addMember": {
        "@type": "method",
        "@input": {
          "organizationNamespace": {
            "@type": "string",
            "@aliases": [
              "organization",
              "org"
            ],
            "@position": 0,
            "@isOptional": true
          },
          "userNamespace": {
            "@type": "string",
            "@aliases": [
              "user"
            ],
            "@position": 1,
            "@isOptional": true
          }
        }
      },
      "removeMember": {
        "@type": "method",
        "@aliases": [
          "rmMember"
        ],
        "@input": {
          "organizationNamespace": {
            "@type": "string",
            "@aliases": [
              "organization",
              "org"
            ],
            "@position": 0,
            "@isOptional": true
          },
          "userNamespace": {
            "@type": "string",
            "@aliases": [
              "user"
            ],
            "@position": 1,
            "@isOptional": true
          }
        }
      }
    },
    "resources": {
      "@aliases": [
        "resource",
        "res"
      ],
      "@implementation": "./dist/cjs/resources",
      "@isHidden": true,
      "get": {
        "@type": "method",
        "@input": {
          "specifier": {
            "@type": "string",
            "@position": 0
          },
          "throwIfNotFound": {
            "@default": true
          }
        },
        "@output": {
          "resource": {
            "@type": "object",
            "@isOptional": true
          }
        }
      },
      "fetch": {
        "@type": "method",
        "@input": {
          "specifier": {
            "@type": "string",
            "@position": 0
          }
        },
        "@output": {
          "file": {
            "@type": "string"
          },
          "cacheStatus": {
            "@type": "string"
          }
        }
      },
      "publish": {
        "@type": "method",
        "@input": {
          "file": {
            "@type": "string",
            "@position": 0
          },
          "permissionToken": {
            "@type": "string",
            "@isOptional": true
          }
        }
      }
    },
    "admin": {
      "@implementation": "./dist/cjs/admin",
      "@isHidden": true,
      "invokeMethod": {
        "@type": "method",
        "@aliases": [
          "invoke"
        ],
        "@input": {
          "name": {
            "@type": "string",
            "@position": 0
          },
          "arguments": {
            "@type": "object",
            "@isOptional": true,
            "@isSubInput": true
          }
        }
      },
      "permissionTokens": {
        "@implementation": "./dist/cjs/admin-permission-tokens",
        "create": {
          "@type": "method",
          "@input": {
            "permission": {
              "@type": "string",
              "@position": 0
            },
            "parameters": {
              "@type": "object",
              "@isOptional": true,
              "@isSubInput": true
            }
          }
        }
      }
    }
  }
}
