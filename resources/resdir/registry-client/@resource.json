      "@aliases": [
        "resource",
        "res"
      ],
      "@implementation": "./dist/resources",
      "@isHidden": true,
      "fetch": {
        "@type": "method",
        "@input": {
          "specifier": {
            "@type": "string",
            "@position": 0
          }
        },
        "@output": {
          "file": {
            "@type": "string"
          },
          "cacheStatus": {
            "@type": "string"
          }
        }
      },
      "publish": {
        "@type": "method",
        "@input": {
          "file": {
            "@type": "string",
            "@position": 0
          },
          "permissionToken": {
            "@type": "string",
            "@isOptional": true
          }
        }
      }
    },
    "admin": {
      "@implementation": "./dist/admin",
      "@isHidden": true,
      "invokeMethod": {
        "@type": "method",
        "@aliases": [
          "invoke"
        ],
        "@input": {
          "name": {
            "@type": "string",
            "@position": 0
          },
          "arguments": {
            "@type": "object",
            "@isOptional": true,
            "@isSubInput": true
          }
        }
      },
      "permissionTokens": {
        "@implementation": "./dist/admin-permission-tokens",
        "create": {
          "@type": "method",
          "@input": {
            "permission": {
              "@type": "string",
              "@position": 0
            },
            "parameters": {
              "@type": "object",
              "@isOptional": true,
              "@isSubInput": true
            }
          }
        }
      }
    }
  }
}
