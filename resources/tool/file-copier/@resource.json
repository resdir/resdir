{
  "@import": [
    "resdir/resource",
    "1place/js-resource"
  ],
  "id": "tool/file-copier",
  "version": "0.1.6",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "fs-extra": "^5.0.0",
    "is-directory": "^0.3.1",
    "lodash": "^4.17.4",
    "@resdir/console": "^0.1.35"
  },
  "@export": {
    "@implementation": "./dist/cjs",
    "sourceDirectory": {
      "@default": "./"
    },
    "destinationDirectory": {
      "@type": "string"
    },
    "clearDestination": {
      "@default": false
    },
    "files": {
      "@type": "array"
    },
    "run": {
      "@type": "method"
    }
  }
}
