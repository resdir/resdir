{
  "@import": [
    "resdir/resource#^0.1.0",
    "1place/js-resource#^0.1.0"
  ],
  "id": "tool/console",
  "version": "0.1.15",
  "isPublic": true,
  "authors": [
    "Manuel Vila <hi@mvila.me>"
  ],
  "license": "MIT",
  "repository": "https://github.com/resdir/resdir.git",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@resdir/console": "^0.1.24"
  },
  "@export": {
    "@name": "Console",
    "@description": "Manage the console [still in development]",
    "@implementation": "./dist/cjs",
    "print": {
      "@type": "method",
      "@description": "Print a message to the console",
      "@input": {
        "message": {
          "@type": "string",
          "@description": "Message to print",
          "@position": 0,
          "@isOptional": true
        }
      }
    }
  }
}
