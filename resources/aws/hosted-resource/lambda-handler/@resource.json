{
  "@import": ["1place/js-resource"],
  "dependencies": {
    "run-core": "^0.11.39",
    "@resdir/remote-resource-json-rpc-handler": "^0.1.0"
  },
  "transpiler": {
    "format": "esm",
    "runBundler": {
      "@type": "method",
      "@run": "@parent bundler run",
      "@listen": "transpilationOccurred"
    }
  },
  "bundler": {
    "@import": "js/bundler",
    "entry": "./dist/esm/index.js",
    "output": "./dist/bundle.js",
    "target": "node",
    "format": "cjs",
    "reinstallDependencies": true,
    "minify": true
  }
}
