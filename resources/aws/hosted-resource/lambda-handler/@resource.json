{
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "@resdir/json-rpc": "^0.1.0"
    }
  },
  "transpiler": {
    "@import": "1place/esnext-transpiler",
    "destination": "./dist/esm",
    "format": "esm",
    "runBundler": {
      "@type": "method",
      "@run": "@parent bundler run",
      "@listen": "transpilationOccurred"
    }
  },
  "bundler": {
    "@import": "js/bundler",
    "entryFile": "./dist/esm/index.js",
    "bundleFile": "./dist/bundle.js",
    "target": "node",
    "format": "cjs",
    "optimize": true
  }
}
