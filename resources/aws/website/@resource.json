{
  "@import": [
    "resdir/resource",
    "js/resource"
  ],
  "id": "aws/website",
  "version": "0.1.14",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies#^0.1.26",
    "@value": {
      "bytes": "^3.0.0",
      "hasha": "^3.0.0",
      "lodash": "^4.17.4",
      "mime-types": "^2.1.17",
      "minimatch": "^3.0.4",
      "recursive-readdir": "^2.2.1",
      "@resdir/aws-client": "^0.1.1",
      "@resdir/console": "^0.1.16",
      "sleep-promise": "^2.0.0"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler#^0.1.9"
    }
  },
  "@export": {
    "@implementation": "./dist",
    "domainName": {
      "@type": "string"
    },
    "contentDirectory": "./content",
    "indexPage": "index.html",
    "customErrors": {
      "@type": "array"
    },
    "immutableFiles": {
      "@type": "array"
    },
    "aws": {
      "accessKeyId": {
        "@type": "string"
      },
      "secretAccessKey": {
        "@type": "string"
      },
      "region": "us-east-1",
      "cloudFront": {
        "priceClass": "PriceClass_All"
      },
      "acm": {
        "region": "us-east-1"
      }
    },
    "deploy": {
      "@type": "method",
      "@before": "@build"
    }
  }
}
