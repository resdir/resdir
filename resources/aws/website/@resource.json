{
  "@import": ["resdir/resource", "js/resource"],
  "id": "aws/website",
  "version": "0.1.7",
  "files": ["./dist"],
  "dependencies": {
    "@import": "js/npm-dependencies#^0.1.26",
    "@value": {
      "bytes": "^3.0.0",
      "hasha": "^3.0.0",
      "lodash": "^4.17.4",
      "mime-types": "^2.1.17",
      "recursive-readdir": "^2.2.1",
      "@resdir/aws-client": "^0.1.1",
      "@resdir/console": "^0.1.16"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler#^0.1.9"
    }
  },
  "@export": {
    "@implementation": "./dist",
    "domainName": {
      "@type": "string"
    },
    "contentDirectory": "./src",
    "indexPage": "index.html",
    "spa": false,
    "aws": {
      "accessKeyId": {
        "@type": "string"
      },
      "secretAccessKey": {
        "@type": "string"
      },
      "region": "us-east-1",
      "cloudFront": {
        "priceClass": "PriceClass_100"
      }
    },
    "deploy": {
      "@type": "method"
    }
  }
}
