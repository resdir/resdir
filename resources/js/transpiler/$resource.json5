{
  $name: 'js/transpiler',
  $version: '0.1.0',
  $type: 'js/resource',
  $files: ['./dist'],
  dependencies: [
    'babel-core@^6.25.0',
    'babel-plugin-transform-class-properties@^6.24.1',
    'babel-plugin-transform-object-rest-spread@^6.23.0',
    'babel-preset-env@^1.5.2',
    'fs-extra@^3.0.1',
    'is-directory@^0.3.1',
    'lodash@^4.17.4',
    'run-common@^0.6.0',
    {name: 'babel-cli', version: '^6.24.1', type: 'development'}
  ],
  test: {
    $type: 'macro',
    $expression: './tests run',
    $listen: 'after:$test'
  },
  $export: {
    $type: 'js/tool',
    $aliases: ['transpiler', 'babel'],
    $implementation: './dist',
    source: './src',
    destination: './dist',
    extensions: ['.js', '.jsx', '.es6', '.es'],
    transpile: {
      $alias: 'run',
      $type: 'command',
      $parameters: [{$name: 'files', $type: 'string'}],
      $variadic: true
    },
    $options: {
      verbose: false,
      quiet: false,
      debug: false
    }
  }
}
