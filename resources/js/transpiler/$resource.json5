{
  $name: "js/transpiler",
  $version: "0.1.0",
  $type: "js/resource",
  $files: [
    "./dist"
  ],
  dependencies: [
    {
      name: "babel-cli",
      version: "^6.24.1",
      type: "development"
    },
    "babel-core@^6.25.0",
    "babel-plugin-transform-class-properties@^6.24.1",
    "babel-plugin-transform-object-rest-spread@^6.23.0",
    "babel-preset-env@^1.5.2",
    "fs-extra@^3.0.1",
    "is-directory@^0.3.1",
    "lodash@^4.17.4",
    "run-common@^0.6.0"
  ],
  test: {
    $type: "macro",
    $listen: "after:$test",
    $expression: "./tests run"
  },
  $export: {
    $aliases: [
      "transpiler",
      "babel"
    ],
    $type: "js/tool",
    $implementation: "./dist",
    source: "./src",
    destination: "./dist",
    extensions: [
      ".js",
      ".jsx",
      ".es6",
      ".es"
    ],
    transpile: {
      $alias: "run",
      $type: "command",
      $parameter: {
        $name: "files",
        $type: "string"
      },
      $variadic: true
    },
    $options: {
      verbose: false,
      quiet: false,
      debug: false
    }
  }
}