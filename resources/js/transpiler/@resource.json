{
  "@name": "js/transpiler",
  "@version": "0.1.0",
  "@files": ["./dist"],
  "dependencies": {
    "@type": "js/dependencies",
    "@value": [
      {
        "name": "babel-cli",
        "version": "^6.24.1",
        "type": "development"
      },
      "babel-core@^6.25.0",
      "babel-plugin-transform-class-properties@^6.24.1",
      "babel-plugin-transform-object-rest-spread@^6.23.0",
      "babel-preset-env@^1.5.2",
      "fs-extra@^3.0.1",
      "is-directory@^0.3.1",
      "lodash@^4.17.4",
      "@resdir/console@^0.1.0"
    ]
  },
  "_testJSTranspiler": {
    "@type": "macro",
    "@hidden": true,
    "@listen": "after:@test",
    "@expression": "./tests run"
  },
  "@export": {
    "@runtime": "node@>=6.10.0",
    "@implementation": "./dist",
    "@options": {
      "verbose": false,
      "quiet": false,
      "debug": false
    },
    "source": "./src",
    "destination": "./dist",
    "extensions": [".js", ".jsx", ".es6", ".es"],
    "run": {
      "@type": "command",
      "@alias": "run",
      "@parameter": {
        "@type": "string",
        "@name": "files"
      },
      "@variadic": true
    }
  }
}
