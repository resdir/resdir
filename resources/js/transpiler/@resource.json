{
  "@import": "js/esnext-resource",
  "id": "js/transpiler",
  "version": "0.1.10",
  "files": [
    "./dist"
  ],
  "dependencies": [
    "babel-core@^6.25.0",
    "babel-plugin-transform-class-properties@^6.24.1",
    "babel-plugin-transform-object-rest-spread@^6.23.0",
    "babel-plugin-transform-react-jsx@^6.24.1",
    "babel-preset-env@^1.5.2",
    "fs-extra@^3.0.1",
    "is-directory@^0.3.1",
    "lodash@^4.17.4",
    "@resdir/console@^0.1.0"
  ],
  "transpiler": {
    "targets": {
      "node": 8
    }
  },
  "_testJSTranspiler": {
    "@type": "macro",
    "@hidden": true,
    "@listen": "after:@test",
    "@expression": "./tests run"
  },
  "@export": {
    "@parameters": {
      "verbose": false,
      "quiet": false,
      "debug": false
    },
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist",
    "source": "./src",
    "destination": "./dist",
    "extensions": [
      ".js",
      ".jsx",
      ".es6",
      ".es"
    ],
    "targets": {
      "@type": "object",
      "@value": {
        "ie": 11,
        "node": 4
      }
    },
    "transformJSX": false,
    "jsxPragma": {
      "@type": "string"
    },
    "run": {
      "@type": "method",
      "@parameters": {
        "file": {
          "@type": "string",
          "@position": 0
        }
      },
      "@emit": "*:run"
    }
  }
}