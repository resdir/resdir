{
  "@import": [
    "resdir/resource",
    "1place/js-resource"
  ],
  "id": "js/transpiler",
  "version": "0.1.3",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "babel-plugin-lodash": "^3.3.2",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.36",
    "@babel/preset-react": "^7.0.0-beta.36",
    "@babel/preset-stage-3": "^7.0.0-beta.36",
    "fs-extra": "^5.0.0",
    "is-directory": "^0.3.1",
    "lodash": "^4.17.4",
    "@resdir/console": "^0.1.0",
    "@resdir/gitignore-manager": "^0.1.1"
  },
  "@unpublishable": {
    "tests": {
      "@import": "1place/esnext-jest",
      "@directory": "./tests",
      "dependencies": {
        "fs-extra": "^5.0.0",
        "@resdir/file-manager": "^0.1.0",
        "run-core": "^0.11.0",
        "temp-dir": "^1.0.0"
      }
    }
  },
  "@export": {
    "@description": "ESNext transpilation (using Babel)",
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist",
    "source": {
      "@default": "./src"
    },
    "destination": {
      "@default": "./dist"
    },
    "extensions": {
      "@default": [
        ".js",
        ".mjs",
        ".jsx",
        ".es6",
        ".es"
      ]
    },
    "targets": {
      "@default": {
        "node": "6.10",
        "chrome": "50",
        "safari": "10",
        "firefox": "53",
        "edge": "14"
      }
    },
    "format": {
      "@default": "cjs"
    },
    "transformJSX": {
      "@default": false
    },
    "jsxPragma": {
      "@type": "string"
    },
    "run": {
      "@type": "method",
      "@input": {
        "files": {
          "@type": "array",
          "@aliases": [
            "file"
          ],
          "@position": 0,
          "@isOptional": true,
          "@isVariadic": true
        }
      }
    },
    "initialize": {
      "@type": "method",
      "@isHidden": true,
      "@input": {
        "gitignore": {
          "@default": true
        }
      },
      "@listen": "@added"
    }
  }
}
