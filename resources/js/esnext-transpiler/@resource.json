{
  "@import": [
    "resdir/resource",
    "js/resource"
  ],
  "id": "js/esnext-transpiler",
  "version": "0.1.25",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "babel-core": "^6.25.0",
      "babel-plugin-transform-class-properties": "^6.24.1",
      "babel-plugin-transform-object-rest-spread": "^6.23.0",
      "babel-plugin-transform-react-jsx": "^6.24.1",
      "babel-preset-env": "^1.5.2",
      "fs-extra": "^3.0.1",
      "is-directory": "^0.3.1",
      "lodash": "^4.17.4",
      "@resdir/console": "^0.1.0",
      "@resdir/gitignore-manager": "^0.1.1"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "./",
      "targets": {
        "node": 8
      },
      "run": {
        "@listen": [
          "after:@build",
          "after:@fileModified"
        ]
      }
    },
    "tests": {
      "@import": "resdir/esnext-jest",
      "@directory": "./tests",
      "dependencies": {
        "fs-extra": "^3.0.1",
        "@resdir/file-manager": "^0.1.0",
        "run-core": "^0.10.0",
        "temp-dir": "^1.0.0"
      }
    }
  },
  "@export": {
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist",
    "source": "./src",
    "destination": "./dist",
    "extensions": [
      ".js",
      ".jsx",
      ".es6",
      ".es"
    ],
    "targets": {
      "@type": "object",
      "@value": {
        "ie": 11,
        "node": 4
      }
    },
    "transformJSX": false,
    "jsxPragma": {
      "@type": "string"
    },
    "run": {
      "@type": "method",
      "@parameters": {
        "file": {
          "@type": "string",
          "@position": 0
        }
      }
    },
    "@initialize": {
      "@type": "method",
      "@parameters": {
        "gitignore": true
      }
    }
  }
}
