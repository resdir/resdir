{
  "@import": [
    "resdir/resource",
    "js/resource"
  ],
  "id": "js/esnext-transpiler",
  "version": "0.1.42",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "babel-core": "^6.25.0",
      "babel-plugin-transform-class-properties": "^6.24.1",
      "babel-plugin-transform-object-rest-spread": "^6.23.0",
      "babel-plugin-transform-react-jsx": "^6.24.1",
      "babel-preset-env": "^1.5.2",
      "fs-extra": "^3.0.1",
      "is-directory": "^0.3.1",
      "lodash": "^4.17.4",
      "@resdir/console": "^0.1.0",
      "@resdir/gitignore-manager": "^0.1.1"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler"
    },
    "tests": {
      "@import": "1place/esnext-jest",
      "@directory": "./tests",
      "dependencies": {
        "fs-extra": "^3.0.1",
        "@resdir/file-manager": "^0.1.0",
        "run-core": "^0.10.0",
        "temp-dir": "^1.0.0"
      }
    }
  },
  "@export": {
    "@description": "ESNext transpilation (using Babel)",
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist",
    "source": {
      "@default": "./src"
    },
    "destination": {
      "@default": "./dist"
    },
    "extensions": {
      "@default": [
        ".js",
        ".jsx",
        ".es6",
        ".es"
      ]
    },
    "targets": {
      "@type": "object",
      "@default": {
        "ie": 11,
        "node": 4
      }
    },
    "transformJSX": {
      "@default": false
    },
    "jsxPragma": {
      "@type": "string"
    },
    "run": {
      "@type": "method",
      "@input": {
        "files": {
          "@type": "array",
          "@aliases": [
            "file"
          ],
          "@position": 0,
          "@isOptional": true,
          "@isVariadic": true
        }
      },
      "@listen": [
        "@build",
        "@fileModified"
      ]
    },
    "initialize": {
      "@type": "method",
      "@isHidden": true,
      "@input": {
        "gitignore": {
          "@default": true
        }
      },
      "@listen": "@added"
    }
  }
}
