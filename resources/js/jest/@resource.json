{
  "@type": "js/esnext-resource",
  "@name": "js/jest",
  "@version": "0.1.1",
  "@files": ["./dist"],
  "dependencies": ["jest@^20.0.4", "regenerator-runtime@^0.11.0", "@resdir/console@^0.1.0"],
  "@export": {
    "@implementation": "./dist",
    "@publishable": false,
    "@options": {
      "verbose": false,
      "quiet": false,
      "debug": false
    },
    "dependencies": {
      "@type": "js/dependencies"
    },
    "roots": {
      "@type": "array"
    },
    "testEnvironment": {
      "@type": "string"
    },
    "run": {
      "@type": "command",
      "@parameter": {
        "testPathPattern": {
          "@type": "string"
        }
      }
    }
  }
}
