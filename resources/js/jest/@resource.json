{
  "@name": "js/jest",
  "@version": "0.1.0",
  "@type": "js/esnext-resource",
  "@files": [
    "./dist"
  ],
  "dependencies": [
    "jest@^20.0.4",
    "run-common@^0.6.1"
  ],
  "@export": {
    "@aliases": [
      "jest",
      "test",
      "tests"
    ],
    "@type": "js/tool",
    "@implementation": "./dist",
    "roots": {
      "@type": "array"
    },
    "testEnvironment": {
      "@type": "string"
    },
    "run": {
      "@alias": "test",
      "@type": "command"
    }
  }
}