{
  "@type": "js/esnext-resource",
  "@name": "js/jest",
  "@version": "0.1.0",
  "@files": ["./dist"],
  "dependencies": ["jest@^20.0.4", "@resdir/console@^0.1.0"],
  "@export": {
    "@implementation": "./dist",
    "dependencies": {
      "@type": "js/dependencies"
    },
    "roots": {
      "@type": "array"
    },
    "testEnvironment": {
      "@type": "string"
    },
    "run": {
      "@type": "command",
      "@alias": "test",
      "@parameter": {
        "@type": "string",
        "@name": "testPathPattern"
      },
      "@listen": "after:@test"
    }
  }
}
