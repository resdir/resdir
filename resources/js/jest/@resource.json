{
  "@name": "js/jest",
  "@version": "0.1.0",
  "@type": "js/esnext-resource",
  "@files": ["./dist"],
  "dependencies": ["jest@^20.0.4", "run-common@^0.6.1"],
  "@export": {
    "@aliases": ["jest", "test", "tests"],
    "@implementation": "./dist",
    "dependencies": {"@type": "js/dependencies"},
    "roots": {
      "@type": "array"
    },
    "testEnvironment": {
      "@type": "string"
    },
    "run": {
      "@type": "command",
      "@alias": "test",
      "@parameters": [
        {
          "@name": "testPathPattern",
          "@type": "string"
        }
      ],
      "@listen": "after:@test"
    }
  }
}
