{
  "@import": "js/esnext-resource",
  "id": "js/resource",
  "version": "0.1.8",
  "files": [
    "./dist"
  ],
  "dependencies": [
    "fs-extra@^4.0.0",
    "@resdir/gitignore-manager@^0.1.0"
  ],
  "transpiler": {
    "targets": {
      "node": 8
    }
  },
  "@export": {
    "@import": "resdir/resource",
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist",
    "dependencies": {
      "@import": "js/dependencies"
    },
    "_createJSResource": {
      "@type": "method",
      "@parameters": {
        "name": {
          "@type": "string",
          "@position": 0
        }
      },
      "@hidden": true,
      "@listen": "after:@create"
    }
  }
}