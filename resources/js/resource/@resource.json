{
  "@name": "js/resource",
  "@version": "0.1.0",
  "@files": ["./dist"],
  "transpiler": {
    "@type": "js/transpiler",
    "run": {
      "@listen": ["after:@build", "after:@fileModified"]
    }
  },
  "dependencies": {
    "@type": "js/dependencies",
    "@value": ["fs-extra@^4.0.0", "@resdir/gitignore-manager@^0.1.0"]
  },
  "@export": {
    "@runtime": "node@>=6.10.0",
    "@implementation": "./dist",
    "dependencies": {
      "@type": "js/dependencies"
    },
    "_createJSResource": {
      "@type": "command",
      "@parameter": {
        "name": {
          "@type": "string"
        }
      },
      "@hidden": true,
      "@listen": "after:@create"
    }
  }
}
