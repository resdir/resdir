{
  "@import": "resdir/resource",
  "id": "js/resource",
  "version": "0.1.26",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "fs-extra": "^4.0.0",
      "@resdir/gitignore-manager": "^0.1.0"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler"
    }
  },
  "@export": {
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist",
    "initialize": {
      "@type": "method",
      "@isHidden": true,
      "@input": {
        "gitignore": {
          "@default": true
        }
      },
      "@listen": "@created"
    }
  }
}
