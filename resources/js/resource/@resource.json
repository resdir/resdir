{
  "@import": [
    "resdir/resource",
    "./"
  ],
  "id": "js/resource",
  "version": "0.1.36",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "fs-extra": "^5.0.0",
    "@resdir/gitignore-manager": "^0.1.0"
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "js/transpiler",
      "run": {
        "@listen": [
          "@build",
          "@fileModified"
        ]
      }
    }
  },
  "@export": {
    "@description": "A starting point for resources implemented in JavaScript",
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist/cjs",
    "dependencies": {
      "@import": "js/npm-dependencies"
    },
    "onCreated": {
      "@type": "method",
      "@isHidden": true,
      "@input": {
        "generateGitignore": {
          "@default": true
        }
      },
      "@listen": "@created"
    }
  }
}
