{
  "@import": "resdir/resource",
  "id": "js/npm-dependencies",
  "version": "0.1.45",
  "isPublic": true,
  "files": [
    "./dist/bundle.js"
  ],
  "@unpublishable": {
    "dependencies": {
      "@import": "js/npm-dependencies",
      "@value": {
        "@resdir/console": "^0.1.0",
        "@resdir/file-manager": "^0.1.0",
        "@resdir/package-manager": "^0.1.0",
        "@resdir/version-range": "^0.1.0",
        "lodash": "^4.17.4"
      }
    },
    "transpiler": {
      "@import": "1place/esnext-transpiler",
      "destination": "./dist/esm",
      "format": "esm",
      "runBundler": {
        "@type": "method",
        "@run": "@parent bundler run",
        "@listen": "transpilationOccurred"
      }
    },
    "bundler": {
      "@import": "js/bundler",
      "entry": "./dist/esm/index.js",
      "output": "./dist/bundle.js",
      "target": "node",
      "format": "cjs",
      "reinstallDependencies": true,
      "minify": true
    },
    "tests": {
      "@import": "1place/esnext-jest",
      "@directory": "./tests",
      "dependencies": {
        "fs-extra": "^5.0.0",
        "@resdir/file-manager": "^0.1.0",
        "run-core": "^0.11.0",
        "temp-dir": "^1.0.0"
      }
    }
  },
  "@export": {
    "@type": "object",
    "@description": "npm package dependencies",
    "@aliases": [
      "dependencies",
      "dependency",
      "deps",
      "dep"
    ],
    "@examples": [
      {
        "fs-extra": "^4.0.2",
        "lodash": "^4.17.4"
      }
    ],
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist/bundle.js",
    "add": {
      "@type": "method",
      "@description": "Add a new dependency",
      "@examples": [
        "dependencies add fs-extra",
        "deps add lodash@^4",
        "deps add lodash@4.17.4",
        "deps add moment react"
      ],
      "@input": {
        "specifiers": {
          "@type": "array",
          "@description": "npm package specifiers",
          "@position": 0,
          "@isVariadic": true,
          "@examples": [
            "fs-extra",
            "lodash@^4",
            "lodash@4.17.4",
            "moment react"
          ]
        },
        "production": {
          "@type": "boolean",
          "@aliases": [
            "prod"
          ],
          "@isOptional": true,
          "@isHidden": true
        },
        "development": {
          "@type": "boolean",
          "@aliases": [
            "dev"
          ],
          "@isOptional": true,
          "@isHidden": true
        },
        "peer": {
          "@type": "boolean",
          "@isOptional": true,
          "@isHidden": true
        },
        "optional": {
          "@type": "boolean",
          "@isOptional": true,
          "@isHidden": true
        }
      }
    },
    "remove": {
      "@type": "method",
      "@description": "Remove an existing dependency",
      "@aliases": [
        "rm"
      ],
      "@input": {
        "names": {
          "@type": "array",
          "@position": 0,
          "@isVariadic": true
        }
      }
    },
    "install": {
      "@type": "method",
      "@description": "Install all dependencies",
      "@listen": "@install"
    },
    "update": {
      "@type": "method",
      "@description": "Update dependencies to their last semantically compatible version"
    },
    "count": {
      "@type": "method",
      "@description": "Count the number of existing dependencies",
      "@output": {
        "@type": "number"
      }
    },
    "includes": {
      "@type": "method",
      "@description": "Determine whether a certain dependency is included",
      "@input": {
        "name": {
          "@type": "string",
          "@position": 0
        }
      },
      "@output": {
        "@type": "boolean"
      }
    },
    "list": {
      "@type": "method",
      "@description": "List existing dependencies",
      "@aliases": [
        "ls"
      ]
    },
    "updatePackageFile": {
      "@type": "method",
      "@isHidden": true
    }
  }
}
