{
  "@import": "resdir/resource",
  "id": "js/npm-dependencies",
  "version": "0.1.20",
  "files": [
    "bundle.js"
  ],
  "@unpublishable": {
    "builder": {
      "@implementation": "./dist/builder.js",
      "run": {
        "@type": "macro",
        "@listen": [
          "after:@build",
          "after:@fileModified"
        ],
        "@expression": [
          "transpiler run",
          "bundle"
        ]
      },
      "transpiler": {
        "@import": "js/esnext-transpiler",
        "targets": {
          "node": 8
        }
      },
      "bundle": {
        "@type": "method"
      }
    },
    "tests": {
      "@import": "resdir/esnext-jest",
      "@directory": "./tests",
      "dependencies": {
        "fs-extra": "^3.0.1",
        "@resdir/file-manager": "^0.1.0",
        "run-core": "^0.10.0",
        "temp-dir": "^1.0.0"
      }
    }
  },
  "@export": {
    "@type": "object",
    "@aliases": [
      "dependencies",
      "dependency",
      "deps",
      "dep"
    ],
    "@runtime": "node#>=6.10.0",
    "@implementation": "./bundle.js",
    "add": {
      "@type": "method",
      "@parameters": {
        "specifier": {
          "@type": "string",
          "@position": 0
        },
        "production": {
          "@aliases": [
            "prod"
          ],
          "@value": false
        },
        "development": {
          "@aliases": [
            "dev"
          ],
          "@value": false
        },
        "peer": false,
        "optional": false
      }
    },
    "remove": {
      "@type": "method",
      "@aliases": [
        "rm"
      ],
      "@parameters": {
        "name": {
          "@type": "string",
          "@position": 0
        }
      }
    },
    "install": {
      "@type": "method",
      "@listen": "after:@install"
    },
    "count": {
      "@type": "method"
    },
    "includes": {
      "@type": "method",
      "@parameters": {
        "name": {
          "@type": "string",
          "@position": 0
        }
      }
    },
    "list": {
      "@type": "method",
      "@aliases": [
        "ls"
      ]
    },
    "updatePackageFile": {
      "@type": "method"
    }
  }
}
