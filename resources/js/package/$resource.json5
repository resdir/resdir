{
  $name: 'js/package',
  $aliases: ['package', 'pkg'],
  $version: '0.1.0',
  $type: 'tool',
  $runtime: 'node@>=6.10.0',
  $implementation: './dist',
  name: {$type: 'string'},
  files: {$type: 'array'},
  main: {
    $implementation: './dist/main',
    es5: {$type: 'string'},
    es6: {$type: 'string'}
  },
  dependencies: {$type: 'js/dependencies'},
  $options: {
    verbose: false,
    quiet: false,
    debug: false
  },
  updatePackageFile: {$type: 'method'}
}
