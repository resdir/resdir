{
  "@import": [
    "resdir/resource",
    "1place/js-resource"
  ],
  "id": "js/npm-package",
  "version": "0.1.41",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.4",
    "@resdir/console": "^0.1.0",
    "@resdir/gitignore-manager": "^0.1.0",
    "@resdir/package-manager": "^0.1.0",
    "@resdir/version": "^0.1.1"
  },
  "@unpublishable": {
    "tests": {
      "@import": "1place/jest",
      "@directory": "./tests",
      "dependencies": {
        "fs-extra": "^5.0.0",
        "@resdir/file-manager": "^0.1.0",
        "run-core": "^0.11.0",
        "temp-dir": "^1.0.0"
      }
    }
  },
  "@export": {
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist/cjs",
    "name": {
      "@type": "string"
    },
    "version": {
      "@type": "string",
      "@implementation": "./dist/cjs/version-property",
      "bump": {
        "@type": "method",
        "@input": {
          "major": {
            "@type": "boolean",
            "@isOptional": true
          },
          "minor": {
            "@type": "boolean",
            "@isOptional": true
          },
          "patch": {
            "@type": "boolean",
            "@isOptional": true
          }
        }
      }
    },
    "description": {
      "@type": "string"
    },
    "author": {
      "@type": "string"
    },
    "contributors": {
      "@type": "array"
    },
    "repository": {
      "@type": "string"
    },
    "license": {
      "@type": "string"
    },
    "files": {
      "@type": "array"
    },
    "main": {
      "@type": "string"
    },
    "module": {
      "@type": "string"
    },
    "bin": {
      "@type": "object"
    },
    "preferGlobal": {
      "@type": "boolean"
    },
    "dependencies": {
      "@import": "js/npm-dependencies"
    },
    "isJSNPMPackageResource": true,
    "publish": {
      "@type": "method",
      "@input": {
        "major": {
          "@type": "boolean",
          "@isOptional": true
        },
        "minor": {
          "@type": "boolean",
          "@isOptional": true
        },
        "patch": {
          "@type": "boolean",
          "@isOptional": true
        },
        "access": {
          "@type": "string",
          "@isOptional": true
        }
      }
    },
    "updatePackageFile": {
      "@type": "method",
      "@listen": [
        "@build",
        "@saved"
      ]
    },
    "initialize": {
      "@type": "method",
      "@isHidden": true,
      "@input": {
        "name": {
          "@type": "string",
          "@position": 0,
          "@isOptional": true
        },
        "version": {
          "@type": "string",
          "@aliases": [
            "v"
          ],
          "@isOptional": true
        },
        "gitignore": {
          "@default": true
        }
      },
      "@listen": "@created"
    }
  }
}
