{
  "@import": [
    "resdir/resource",
    "js/resource"
  ],
  "id": "js/npm-package",
  "version": "0.1.21",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "packages": {
      "fs-extra": "^4.0.0",
      "lodash": "^4.17.4",
      "@resdir/console": "^0.1.0",
      "@resdir/gitignore-manager": "^0.1.0",
      "@resdir/package-manager": "^0.1.0",
      "@resdir/version": "^0.1.1"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "js/esnext-transpiler",
      "targets": {
        "node": 8
      },
      "run": {
        "@listen": [
          "after:@build",
          "after:@fileModified"
        ]
      }
    },
    "tests": {
      "@import": "resdir/esnext-jest",
      "@directory": "./tests",
      "dependencies": {
        "fs-extra": "^3.0.1",
        "@resdir/file-manager": "^0.1.0",
        "run-core": "^0.10.0",
        "temp-dir": "^1.0.0"
      }
    }
  },
  "@export": {
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist",
    "name": {
      "@type": "string"
    },
    "version": {
      "@type": "string",
      "@implementation": "./dist/version-property",
      "bump": {
        "@type": "method",
        "@parameters": {
          "major": false,
          "minor": false,
          "patch": false
        }
      }
    },
    "description": {
      "@type": "string"
    },
    "author": {
      "@type": "string"
    },
    "contributors": {
      "@type": "array"
    },
    "repository": {
      "@type": "string"
    },
    "license": {
      "@type": "string"
    },
    "files": {
      "@type": "array"
    },
    "main": {
      "@type": "string"
    },
    "bin": {
      "@type": "object"
    },
    "preferGlobal": {
      "@type": "boolean"
    },
    "dependencies": {
      "@import": "js/npm-dependencies"
    },
    "isJSNPMPackageResource": true,
    "publish": {
      "@type": "method",
      "@parameters": {
        "major": false,
        "minor": false,
        "patch": false,
        "access": {
          "@type": "string"
        }
      }
    },
    "updatePackageFile": {
      "@type": "method",
      "@listen": [
        "after:@build",
        "after:@save"
      ]
    },
    "@initialize": {
      "@type": "method",
      "@parameters": {
        "name": {
          "@type": "string",
          "@position": 0
        },
        "version": {
          "@type": "string",
          "@aliases": [
            "v"
          ]
        },
        "gitignore": true
      }
    }
  }
}
