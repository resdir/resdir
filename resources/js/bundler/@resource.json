{
  "@import": "resdir/resource",
  "id": "js/bundler",
  "version": "0.1.12",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@import": "js/npm-dependencies",
    "@value": {
      "bytes": "^3.0.0",
      "fs-extra": "^3.0.1",
      "@resdir/console": "^0.1.0",
      "rollup": "^0.53.3",
      "rollup-plugin-commonjs": "^8.2.6",
      "rollup-plugin-json": "^2.3.0",
      "rollup-plugin-node-globals": "^1.1.0",
      "rollup-plugin-node-resolve": "^3.0.0",
      "rollup-plugin-replace": "^2.0.0",
      "rollup-plugin-uglify": "^3.0.0",
      "uglify-es": "^3.3.7"
    }
  },
  "@unpublishable": {
    "transpiler": {
      "@import": "1place/esnext-transpiler"
    }
  },
  "@export": {
    "@description": "JavaScript module bundler (using Rollup)",
    "@runtime": "node#>=6.10.0",
    "@implementation": "./dist",
    "entryFile": {
      "@type": "string"
    },
    "bundleFile": {
      "@type": "string"
    },
    "target": {
      "@default": "node"
    },
    "format": {
      "@default": "cjs"
    },
    "name": {
      "@type": "string"
    },
    "replacements": {
      "@type": "object"
    },
    "reinstallDependencies": {
      "@default": false
    },
    "minify": {
      "@default": false
    },
    "run": {
      "@type": "method"
    }
  }
}
