{
  "@import": "js/resource",
  "dependencies": {
    "sleep-promise": "^2.0.0"
  },
  "bundler": {
    "@import": "..",
    "entry": "./src/index.js",
    "output": "./dist/bundle.js",
    "target": "node",
    "format": "cjs",
    "fileWatcher": {
      "@import": "tool/file-watcher",
      "files": [
        "./src/**"
      ],
      "onFileModified": {
        "@type": "method",
        "@run": "@parent run",
        "@listen": "fileModified"
      }
    }
  }
}
