{
  "@type": "js/esnext-resource",
  "@name": "js/esnext-package",
  "@version": "0.1.0",
  "@files": ["./dist"],
  "@export": {
    "@types": ["js/package"],
    "@implementation": "./dist",
    "transpiler": {
      "@type": "js/transpiler",
      "run": {
        "@listen": ["after:@build", "after:@fileModified"]
      }
    },
    "_createJSESNextPackage": {
      "@type": "command",
      "@listen": "after:@create",
      "@hidden": true
    }
  }
}
