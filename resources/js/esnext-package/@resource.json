{
  "@import": "js/esnext-resource",
  "@name": "js/esnext-package",
  "@version": "0.1.2",
  "@files": ["./dist"],
  "dependencies": ["@resdir/gitignore-manager@^0.1.0"],
  "@export": {
    "@import": "js/package",
    "@code": "./dist",
    "transpiler": {
      "@import": "js/transpiler",
      "run": {
        "@listen": ["after:@build", "after:@fileModified"]
      }
    },
    "_createJSESNextPackage": {
      "@type": "method",
      "@hidden": true,
      "@parameters": {
        "name": {"@type": "string", "@position": 0}
      },
      "@listen": "after:@create"
    }
  }
}
