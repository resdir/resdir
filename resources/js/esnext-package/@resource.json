{
  "@type": "js/esnext-resource",
  "@name": "js/esnext-package",
  "@version": "0.1.0",
  "@files": [
    "./dist"
  ],
  "dependencies": [
    "@resdir/gitignore-manager@^0.1.0"
  ],
  "@export": {
    "@type": "js/package",
    "@implementation": "./dist",
    "transpiler": {
      "@type": "js/transpiler",
      "run": {
        "@listen": [
          "after:@build",
          "after:@fileModified"
        ]
      }
    },
    "_createJSESNextPackage": {
      "@type": "command",
      "@hidden": true,
      "@parameter": {
        "name": {
          "@type": "string"
        }
      },
      "@listen": "after:@create"
    }
  }
}