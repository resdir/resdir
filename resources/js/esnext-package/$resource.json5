{
  $name: 'js/esnext-package',
  $version: '0.1.0',
  $type: 'js/package',
  $runtime: 'node@>=6.10.0',
  $implementation: './dist',
  sourceDirectory: './src',
  distributionDirectory: './dist',
  files: ['./dist'],
  main: './dist',
  transpilation: {
    fileExtensions: ['.js', '.jsx', '.es6', '.es']
  },
  build: {
    $type: 'command',
    $parameters: [{$name: 'files', $type: 'string'}],
    $variadic: true,
  }
}
