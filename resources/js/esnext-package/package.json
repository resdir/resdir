{
  "name": "@resdir/js-esnext-package",
  "description": "Resdir js/esnext-package resource",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "test": "jest"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 6
          },
          "loose": true,
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "sourceMaps": "inline"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "fs-extra": "^3.0.1",
    "is-directory": "^0.3.1",
    "lodash": "^4.17.4",
    "run-common": "file:../../../run/common"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "jest": "^20.0.4"
  }
}
