{
  "@import": "js/esnext-resource",
  id: "js/esnext-package",
  version: "0.1.8",
  files: [
    "./dist"
  ],
  dependencies: [
    "@resdir/gitignore-manager@^0.1.0"
  ],
  "@export": {
    "@import": "js/package",
    "@implementation": "./dist",
    "@initialize": {
      "@type": "method"
    },
    transpiler: {
      "@import": "js/transpiler",
      run: {
        "@listen": [
          "after:@build",
          "after:@fileModified"
        ]
      }
    }
  }
}