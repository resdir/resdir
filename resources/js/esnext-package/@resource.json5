{
  "@import": "js/esnext-resource",
  id: "js/esnext-package",
  version: "0.1.6",
  files: [
    "./dist"
  ],
  dependencies: [
    "@resdir/gitignore-manager@^0.1.0"
  ],
  "@export": {
    "@import": "js/package",
    "@implementation": "./dist",
    transpiler: {
      "@import": "js/transpiler",
      run: {
        "@listen": [
          "after:@build",
          "after:@fileModified"
        ]
      }
    },
    _createJSESNextPackage: {
      "@type": "method",
      "@parameters": {
        name: {
          "@type": "string",
          "@position": 0
        }
      },
      "@hidden": true,
      "@listen": "after:@create"
    }
  }
}