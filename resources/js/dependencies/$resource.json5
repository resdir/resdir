{
  $name: 'js/dependencies',
  $alias: ['dependencies', 'dependency', 'deps', 'dep'],
  $version: '0.1.0',
  $type: 'tool',
  $runtime: 'node@>=6.10.0',
  $implementation: './dist',
  $options: {
    verbose: false,
    quiet: false,
    debug: false
  },
  add: {
    $type: 'command',
    $parameters: [{$name: 'package', $type: 'string'}],
    $variadic: true,
    $options: {
      production: {
        $alias: 'prod',
        $value: false
      },
      development: {
        $alias: 'dev',
        $value: false
      },
      peer: false,
      optional: false
    }
  },
  remove: {
    $alias: 'rm',
    $type: 'command',
    $parameters: [{$name: 'package', $type: 'string'}],
    $variadic: true
  },
  count: {
    $type: 'command'
  },
  includes: {
    $type: 'command',
    $parameter: [{$name: 'package', $type: 'string'}]
  },
  updatePackageFile: {$type: 'method'}
}
