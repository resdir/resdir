{
  "@import": "resdir/resource",
  id: "js/dependencies",
  version: "0.1.11",
  files: [
    "bundle.js"
  ],
  builder: {
    "@implementation": "./dist/builder.js",
    "@publishable": false,
    run: {
      "@type": "macro",
      "@listen": [
        "after:@build",
        "after:@fileModified"
      ],
      "@expression": [
        "transpiler run",
        "bundle"
      ]
    },
    transpiler: {
      "@import": "js/transpiler",
      targets: {
        node: 8
      }
    },
    bundle: {
      "@type": "method"
    }
  },
  _testJSDependencies: {
    "@type": "macro",
    "@hidden": true,
    "@listen": "after:@test",
    "@expression": "./tests run"
  },
  "@export": {
    "@aliases": [
      "dependencies",
      "dependency",
      "deps",
      "dep"
    ],
    "@runtime": "node#>=6.10.0",
    "@implementation": "./bundle.js",
    add: {
      "@type": "method",
      "@parameters": {
        specifier: {
          "@type": "string",
          "@position": 0
        },
        production: {
          "@aliases": [
            "prod"
          ],
          "@value": false
        },
        development: {
          "@aliases": [
            "dev"
          ],
          "@value": false
        },
        peer: false,
        optional: false
      }
    },
    remove: {
      "@type": "method",
      "@aliases": [
        "rm"
      ],
      "@parameters": {
        name: {
          "@type": "string",
          "@position": 0
        }
      }
    },
    install: {
      "@type": "method",
      "@listen": "after:@install"
    },
    count: {
      "@type": "method"
    },
    includes: {
      "@type": "method",
      "@parameters": {
        name: {
          "@type": "string",
          "@position": 0
        }
      }
    },
    updatePackageFile: {
      "@type": "method"
    }
  }
}