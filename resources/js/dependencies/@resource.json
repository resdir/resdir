{
  "@name": "js/dependencies",
  "@version": "0.1.0",
  "@files": ["./dist", "package.json"],
  "test": {
    "@type": "macro",
    "@expression": "./tests run",
    "@listen": "after:@test"
  },
  "@export": {
    "@alias": ["dependencies", "dependency", "deps", "dep"],
    "@runtime": "node@>=6.10.0",
    "@implementation": "./dist",
    "add": {
      "@type": "command",
      "@parameters": [
        {
          "@name": "package",
          "@type": "string"
        }
      ],
      "@variadic": true,
      "@options": {
        "production": {
          "@alias": "prod",
          "@value": false
        },
        "development": {
          "@alias": "dev",
          "@value": false
        },
        "peer": false,
        "optional": false
      }
    },
    "remove": {
      "@alias": "rm",
      "@type": "command",
      "@parameters": [
        {
          "@name": "package",
          "@type": "string"
        }
      ],
      "@variadic": true
    },
    "install": {
      "@type": "command"
    },
    "count": {
      "@type": "command"
    },
    "includes": {
      "@type": "command",
      "@parameter": [
        {
          "@name": "package",
          "@type": "string"
        }
      ]
    },
    "updatePackageFile": {
      "@type": "command"
    },
    "@options": {
      "verbose": false,
      "quiet": false,
      "debug": false
    }
  }
}
