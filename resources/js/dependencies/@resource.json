{
  "@name": "js/dependencies",
  "@version": "0.1.3",
  "@files": ["bundle.js"],
  "_testJSDependencies": {
    "@type": "macro",
    "@expression": "./tests run",
    "@listen": "after:@test",
    "@hidden": true
  },
  "@export": {
    "@alias": ["dependencies", "dependency", "deps", "dep"],
    "@runtime": "node@>=6.10.0",
    "@code": "./bundle.js",
    "add": {
      "@type": "method",
      "@parameters": {
        "specifier": {"@type": "string", "@position": 0},
        "production": {
          "@alias": "prod",
          "@value": false
        },
        "development": {
          "@alias": "dev",
          "@value": false
        },
        "peer": false,
        "optional": false
      }
    },
    "remove": {
      "@alias": "rm",
      "@type": "method",
      "@parameters": {
        "name": {"@type": "string", "@position": 0}
      }
    },
    "install": {
      "@type": "method",
      "@listen": "after:@install"
    },
    "count": {
      "@type": "method"
    },
    "includes": {
      "@type": "method",
      "@parameters": {
        "name": {"@type": "string", "@position": 0}
      }
    },
    "updatePackageFile": {
      "@type": "method"
    },
    "@parameters": {
      "verbose": false,
      "quiet": false,
      "debug": false
    }
  }
}
