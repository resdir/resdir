{
  "@import": [
    "resdir/resource#^0.1.0",
    "1place/js-resource#^0.1.0"
  ],
  "id": "shell/concurrently",
  "version": "0.1.1",
  "isPublic": true,
  "authors": [
    "Manuel Vila <hi@mvila.me>"
  ],
  "license": "MIT",
  "repository": "https://github.com/resdir/resdir.git",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "concurrently": "^5.0.0",
    "@resdir/error": "^0.1.3"
  },
  "@export": {
    "@name": "Concurrently",
    "@description": "Run multiple commands concurrently",
    "@implementation": "./dist/cjs",
    "commands": {
      "@type": "array",
      "@description": "An array of strings specifying the commands to run",
      "@default": []
    },
    "run": {
      "@type": "method",
      "@description": "Run the commands concurrently"
    }
  }
}
