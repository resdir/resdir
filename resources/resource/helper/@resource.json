{
  "@import": [
    "resdir/resource",
    "1place/js-resource"
  ],
  "id": "resource/helper",
  "version": "0.1.12",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "indent-string": "^3.2.0",
    "lodash": "^4.17.4",
    "@resdir/console": "^0.1.31",
    "@resdir/error": "^0.1.0",
    "@resdir/resource-specifier": "^0.1.10"
  },
  "@export": {
    "@implementation": "./dist/cjs",
    "create": {
      "@type": "method",
      "@input": {
        "typeOrSpecifier": {
          "@type": "string",
          "@position": 0
        }
      }
    },
    "add": {
      "@type": "method",
      "@input": {
        "resourcePtr": {
          "@type": "pointer",
          "@position": 0
        },
        "typeOrSpecifier": {
          "@type": "string",
          "@position": 1
        },
        "key": {
          "@type": "string",
          "@position": 2
        }
      }
    },
    "remove": {
      "@type": "method",
      "@aliases": [
        "rm"
      ],
      "@input": {
        "resourcePtr": {
          "@type": "pointer",
          "@position": 0
        },
        "key": {
          "@type": "string",
          "@position": 1
        }
      }
    },
    "normalize": {
      "@type": "method",
      "@input": {
        "resourcePtr": {
          "@type": "pointer",
          "@position": 0
        },
        "format": {
          "@type": "string",
          "@default": "JSON"
        }
      }
    },
    "help": {
      "@type": "method",
      "@input": {
        "resourcePtr": {
          "@type": "pointer",
          "@position": 0
        },
        "keys": {
          "@type": "array",
          "@position": 1,
          "@isOptional": true,
          "@isVariadic": true
        },
        "showNative": {
          "@type": "boolean",
          "@aliases": [
            "native"
          ],
          "@isOptional": true
        }
      }
    }
  }
}
