{
  "@import": [
    "resdir/resource",
    "1place/js-resource"
  ],
  "id": "website/freezer",
  "version": "0.1.4",
  "isPublic": true,
  "files": [
    "./dist"
  ],
  "dependencies": {
    "fs-extra": "^5.0.0",
    "@resdir/console": "^0.1.35",
    "rev-hash": "^2.0.0"
  },
  "@export": {
    "@implementation": "./dist/cjs",
    "sourceDirectory": {
      "@type": "string"
    },
    "destinationDirectory": {
